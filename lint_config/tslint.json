{
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "adjacent-overload-signatures": true,
    "no-trailing-whitespace": false,
    "compilerOptions": {
      "experimentalDecorators": true
    }
  }
}
